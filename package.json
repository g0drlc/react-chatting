{
  "name": "react-redux-apollo-starter",
  "version": "0.0.1",
  "scripts": {
    "build": "NODE_ENV=production webpack -c --progress --config webpack/config.babel.js",
    "lint": "eslint --ignore-path .gitignore .",
    "prebuild": "rimraf build/*",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node server",
    "start-dev": "NODE_ENV=development nodemon server -w server -w client",
    "watch-client": "node webpack/webpack-dev-server.js",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\""
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "eslint": "3.5.0",
    "eslint-config-richardkall": "5.0.1",
    "nodemon": "1.10.2",
    "react-hot-loader": "3.0.0-beta.3",
    "webpack-dev-middleware": "1.7.0",
    "webpack-hot-middleware": "2.12.2"
  },
  "dependencies": {
    "apollo-client": "^0.5.11",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "6.14.0",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.2",
    "classnames": "^2.2.5",
    "compression": "1.6.2",
    "css-loader": "0.25.0",
    "css-modules-require-hook": "4.0.2",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "graphql": "^0.8.2",
    "graphql-server-express": "^0.4.3",
    "graphql-subscriptions": "^0.2.2",
    "graphql-tag": "^1.1.2",
    "graphql-tools": "^0.8.3",
    "history": "4.0.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^7.1.9",
    "knex": "^0.12.6",
    "moment": "^2.17.0",
    "morgan": "1.7.0",
    "pg": "^6.1.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-loader": "0.13.0",
    "rimraf": "2.5.4",
    "react": "15.3.1",
    "react-apollo": "^0.6.0",
    "react-dom": "15.3.1",
    "react-helmet": "3.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.8.0",
    "redux": "3.6.0",
    "redux-form": "^6.2.1",
    "redux-thunk": "^2.1.0",
    "semantic-ui-react": "^0.61.10",
    "style-loader": "0.13.1",
    "subscriptions-transport-ws": "^0.3.1",
    "url-loader": "0.5.7",
    "validator": "^6.1.0",
    "webpack": "1.13.1",
    "webpack-isomorphic-tools": "^2.6.4"
  },
  "description": "Example of a universal web-app using react, redux, apollo, and hot reloading",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamjking3/react-redux-apollo-boilerplate.git"
  },
  "keywords": [
    "react",
    "universal",
    "redux",
    "apollo",
    "webpack",
    "babel",
    "express",
    "hot",
    "reloading",
    "react-hot-loader",
    "starter",
    "boilerplate"
  ],
  "author": "Adam King https://github.com/adamjking3",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adamjking3/react-redux-apollo-boilerplate/issues"
  },
  "homepage": "https://github.com/adamjking3/react-redux-apollo-boilerplate#readme"
}

{
  "name": "react-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production webpack -c --progress",
    "clean": "rm -rf build/*",
    "lint": "eslint . && stylelint ./client/**/*.css",
    "prebuild": "npm run clean",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node ./server",
    "start-dev": "nodemon ./server --watch ./server",
    "test": "NODE_ENV=test mocha",
    "test-watch": "NODE_ENV=test mocha -R min -w"
  },
  "pre-commit": "lint",
  "devDependencies": {
    "assets-webpack-plugin": "~3.3.0",
    "autoprefixer": "~6.3.3",
    "babel-core": "~6.5.2",
    "babel-eslint": "~5.0.0",
    "babel-loader": "~6.2.4",
    "babel-plugin-react-transform": "~2.0.0",
    "babel-preset-es2015": "~6.5.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "~6.5.0",
    "css-loader": "~0.23.1",
    "css-modules-require-hook": "~3.0.0",
    "eslint": "~2.2.0",
    "eslint-config-richardkall": "~2.3.0",
    "eslint-plugin-react": "~4.1.0",
    "expect": "~1.14.0",
    "expect-jsx": "~2.3.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.8.5",
    "mocha": "~2.4.5",
    "nodemon": "~1.9.1",
    "postcss-loader": "~0.8.1",
    "postcss-nested": "~1.0.0",
    "pre-commit": "~1.1.2",
    "react-addons-test-utils": "~0.14.7",
    "react-transform-hmr": "~1.0.2",
    "style-loader": "~0.13.0",
    "stylelint": "~4.4.0",
    "stylelint-config-richardkall": "~0.1.0",
    "url-loader": "~0.5.7",
    "webpack": "~1.12.14",
    "webpack-dev-middleware": "~1.5.1",
    "webpack-hot-middleware": "~2.7.1"
  },
  "dependencies": {
    "classnames": "~2.2.3",
    "compression": "~1.6.1",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "react": "~0.14.7",
    "react-dom": "~0.14.7",
    "react-redux": "~4.4.0",
    "react-router": "~2.0.0",
    "redux": "~3.3.1"
  }
}

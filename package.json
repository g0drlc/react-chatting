{
  "name": "react-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production webpack -c --progress",
    "lint": "eslint --ignore-path .gitignore . && stylelint client/**/*.css",
    "prebuild": "rm -rf build/*",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node server",
    "start-dev": "nodemon server -w server",
    "test": "NODE_ENV=test mocha",
    "test-watch": "NODE_ENV=test mocha -R min -w"
  },
  "pre-commit": "lint",
  "devDependencies": {
    "babel-eslint": "~6.0.0-beta.6",
    "babel-plugin-react-transform": "~2.0.0",
    "eslint-config-richardkall": "~2.6.0",
    "eslint-plugin-react": "~4.2.3",
    "eslint": "~2.4.0",
    "expect-jsx": "~2.4.0",
    "expect": "~1.15.2",
    "mocha": "~2.4.5",
    "nock": "~7.2.2",
    "nodemon": "~1.9.1",
    "pre-commit": "~1.1.2",
    "react-addons-test-utils": "~0.14.7",
    "react-transform-hmr": "~1.0.4",
    "redux-mock-store": "~1.0.2",
    "stylelint-config-richardkall": "~2.0.0",
    "stylelint": "~5.0.1",
    "webpack-dev-middleware": "~1.5.1",
    "webpack-hot-middleware": "~2.10.0"
  },
  "dependencies": {
    "assets-webpack-plugin": "~3.4.0",
    "autoprefixer": "~6.3.3",
    "babel-core": "~6.7.2",
    "babel-loader": "~6.2.4",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "~6.5.0",
    "babel-register": "~6.7.2",
    "classnames": "~2.2.3",
    "compression": "~1.6.1",
    "css-loader": "~0.23.1",
    "css-modules-require-hook": "~4.0.0",
    "express": "~4.13.4",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.8.5",
    "history": "~2.0.1",
    "isomorphic-fetch": "~2.2.1",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "postcss-loader": "~0.8.2",
    "postcss-nested": "~1.0.0",
    "react": "~0.14.7",
    "react-dom": "~0.14.7",
    "react-redux": "~4.4.1",
    "react-router": "~2.0.1",
    "redux": "~3.3.1",
    "redux-thunk": "~2.0.1",
    "style-loader": "~0.13.0",
    "url-loader": "~0.5.7",
    "webpack": "~1.12.14"
